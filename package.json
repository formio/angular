{
  "name": "angular-formio",
  "version": "2.0.0-alpha.1",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n angular-formio -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "build:demo": "webpack --mode=development",
    "demo": "concurrently --raw \"webpack-dev-server --open\" \"npm run test:watch\"",
    "test": "tsc && karma start",
    "test:watch": "karma start --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/formio/angular-formio"
  },
