{
  "name": "@heybaton/formio-ng",
  "version": "5.3.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build angular-formio && npm run version",
    "build:watch": "ng build angular-formio --watch",
    "build:prod": "ng build angular-formio --configuration production && npm run version",
    "version": "VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\\\",]//g' | tr -d '[[:space:]]') && ./node_modules/replace-in-file/bin/cli.js \"VERSION\" \"$VERSION\" dist/angular-formio/package.json",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n angular-formio -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "publish": "npm run build:prod && npm publish ./dist/angular-formio",
    "test": "ng test",
    "lint": "ng lint",
    "postinstall": "node -e 'var fs=require(`fs`);var fname=`./projects/angular-formio/package.json`;var pjson=require(fname);pjson.version=require(`./package.json`).version;fs.writeFileSync(fname,JSON.stringify(pjson,null, 2))'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heybaton/formio-ng"
  },
  "author": {
    "name": "Travis Tidwell",
    "email": "travis@form.io"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/heybaton/formio-ng/issues"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.4",
    "@angular/animations": "^13.2.3",
    "@angular/cli": "^13.2.4",
    "@angular/common": "^13.2.3",
    "@angular/compiler": "^13.2.3",
    "@angular/compiler-cli": "^13.2.3",
    "@angular/core": "^13.2.3",
    "@angular/elements": "^13.2.3",
    "@angular/forms": "^13.2.3",
    "@angular/platform-browser": "^13.2.3",
    "@angular/platform-browser-dynamic": "^13.2.3",
    "@angular/router": "^13.2.3",
    "@compodoc/compodoc": "^1.1.18",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^17.0.19",
    "codelyzer": "^6.0.2",
    "formiojs": "^4.14.1",
    "jasmine-core": "^4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.2.1",
    "ngx-bootstrap": "^8.0.0",
    "protractor": "~7.0.0",
    "replace-in-file": "^6.3.2",
    "rxjs": "^7.5.4",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "tslint": "~6.1.0",
    "typescript": "~4.4.4",
    "zone.js": "~0.11.4"
  }
}
